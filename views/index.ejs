            <div class="portfolio-card-container">

                <% dataProject.forEach(project => { %>

                    <div class="portfolio-card <%= project.name.toLowerCase() %>">
                        <button class="portfolio-btn-project"><%= project.name.toUpperCase() %></button>

                        <a href="#portfolio">
                            <i id="<%= project.id %>" class="portfolio-watch-project far fa-eye fa-lg"></i>
